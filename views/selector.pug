extends layout

block content
  h1= title
  - let image_names = ['rabbit1.jpg','rabbit2.jpg','rabbit3.jpg','rabbit4.jpg','rabbit5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]

      img(src=`images/${iname}` alt='rabbit' style='width:300px; height:300px;')
      |    
  - }